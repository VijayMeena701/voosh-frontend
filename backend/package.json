    "description": "backend for task manager app",
    "main": "index.ts",
    "scripts": {
        "test": "test",
        "dev": "nodemon",
        "start": "ts-node index.ts",
        "start:prod": "npm run build && node build/index.js",
        "build": "npm run build-ts",
        "clean": "mkdir build",
